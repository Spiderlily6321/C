#include<stdio.h>

int main() {

    int arr[] = {5, 2, 8, 3, 1, 7, 9, 4};


    int size = sizeof(arr) / sizeof(arr[0]);

    int min = arr[0];
    int max = arr[0];
    int i;
   

    for (i = 1; i < size; i++) {
        if(arr[i] < min) {
            min = arr[i];
        } else if (arr[i] > max) {
            max = arr[i];
        }
    }

    printf("Minimum element: %d\n", min);
    printf("Maximum element: %d\n", max);

    return 0;
}
