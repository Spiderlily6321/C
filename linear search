#include <stdio.h>

int main() {

    int arr[] = {5, 2, 8, 3, 1, 7, 9, 4};

    int size = sizeof(arr) / sizeof(arr[0]);

    int key = 7;
    

    int index = -1;  
    
    for (int i = 0; i < size; i++) {
        if (arr[i] == key) {
            index = i;  
            break;      
        }
    }
    

    if (index != -1) {
        printf("Element %d found at index %d\n", key, index);
    } else {
        printf("Element %d not found in the array\n", key);
    }
    
    return 0;
}
